Algoritmo ejercicio6
	Definir nota Como Real;
	Escribir "Digite su nota";
	Leer nota;
	
	Si nota > 4.5 Entonces
		Escribir "Exelente";
	Fin Si	
	Si nota > 4 Y nota <= 4.5 Entonces
		Escribir "Muy bueno";
	Fin Si
	Si nota > 3.5 Y nota <= 4 Entonces
		Escribir "Bueno";
	Fin Si
	Si nota > 3 Y nota <= 3.5 Entonces
		Escribir "Regular";
	Fin Si
	Si nota > 2.5 Y nota <= 3 Entonces
		Escribir "Deficiente";
	Fin Si
FinAlgoritmo
